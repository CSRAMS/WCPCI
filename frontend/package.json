{
    "name": "frontend",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "check": "astro check",
        "format-check": "prettier --check .",
        "build": "astro build && renamer -s --find html --replace html.tera \"./dist/**\" && node cspOutputer.mjs",
        "csp": "node cspOutputer.mjs",
        "preview": "astro preview",
        "astro": "astro",
        "format": "prettier --write .",
        "watch": "npm run build && chokidar \"**/*.astro\" \"**/*.mjs\" \"public/**\" -i \"node_modules\" -i \"src/generated\" -i \"dist\" -c \"npm run build\""
    },
    "dependencies": {
        "@astrojs/check": "^0.9.1",
        "@astrojs/tailwind": "^5.1.0",
        "@codemirror/autocomplete": "^6.18.0",
        "@codemirror/commands": "^6.6.0",
        "@codemirror/lang-cpp": "^6.0.2",
        "@codemirror/lang-go": "^6.0.1",
        "@codemirror/lang-java": "^6.0.1",
        "@codemirror/lang-javascript": "^6.2.2",
        "@codemirror/lang-php": "^6.0.1",
        "@codemirror/lang-python": "^6.1.6",
        "@codemirror/lang-rust": "^6.0.1",
        "@codemirror/language": "^6.10.2",
        "@codemirror/lint": "^6.8.1",
        "@codemirror/search": "^6.5.6",
        "@codemirror/state": "^6.4.1",
        "@codemirror/view": "^6.32.0",
        "@flok-editor/lang-haskell": "^1.0.0-alpha.1",
        "@nextjournal/lang-clojure": "^1.0.0",
        "@plutojl/lang-julia": "^0.12.1",
        "@replit/codemirror-lang-csharp": "^6.2.0",
        "astro": "^4.13.1",
        "astro-icon": "^1.1.0",
        "canvas-confetti": "^1.9.3",
        "chokidar-cli": "^3.0.0",
        "codemirror-lang-r": "^0.1.0-2",
        "codemirror-lang-zig": "^0.1.0",
        "devicon": "^2.16.0",
        "highlight.js": "^11.10.0",
        "katex": "^0.16.11",
        "lorem-ipsum": "^2.0.8",
        "renamer": "^5.0.2",
        "tailwindcss": "^3.4.7",
        "typescript": "^5.5.4"
    },
    "devDependencies": {
        "@fontsource/roboto": "^5.0.14",
        "@fontsource/roboto-mono": "^5.0.18",
        "@iconify-json/tabler": "^1.1.119",
        "@kindspells/astro-shield": "^1.3.6",
        "@types/canvas-confetti": "^1.6.4",
        "@types/katex": "^0.16.7",
        "prettier": "^3.3.3",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "sass": "^1.77.8"
    }
}
